) => {\\n  // add listeners to socket messages so we can re-dispatch them as actions\\n  Object.keys(messageTypes)\\n    .forEach(type => socket.onmessage = (event) => store.dispatch({ type: event.data.type, payload: event.data.payload }))\\n}\\n\\n\\nconst send = function (message) {\\n    waitForConnection(function () {\\n        ws.send(message);\\n    }, 1000);\\n};\\n\\nconst waitForConnection = function (callback, interval) {\\n    if (ws.readyState === 1) {\\n        callback();\\n    } else {\\n        setTimeout(function () {\\n            waitForConnection(callback, interval);\\n        }, interval);\\n    }\\n};\\n\\n\\nconst emit = (type, payload) => send({type: type, payload: payload}) \\n\\nexport {\\n  init,\\n  emit\\n}\\n\\n\\n\\n// WEBPACK FOOTER //\\n// ./actions/websocket.js\"],\"sourceRoot\":\"\"}\n\n/Users/work/go/src/github.com/TimothyJones/chat-client/server/public/app.min-96ccab.js:\n  670  \t\n  671  \t    _this.onJoinClick = (0, _lodash2.default)(_this.onJoinClick, _this);\n  672: \t    _this.checkName = (0, _lodash2.default)(_this.checkName, _this);\n  673  \t    _this.state = { valid: false, name: null };\n  674  \t    return _this;\n  ...\n  685  \t    }\n  686  \t  }, {\n  687: \t    key: 'checkName',\n  688: \t    value: function checkName(event) {\n  689  \t      var name = event.target.value;\n  690  \t      var valid = name && name.length > 0;\n  ...\n  707  \t        'div',\n  708  \t        { style: styles.form },\n  709: \t        _react2.default.createElement('input', { type: 'text', maxLength: '20', placeholder: 'Your name', ref: 'nameInput', onKeyDown: this.checkName,\n  710: \t          onChange: this.checkName\n  711  \t        }),\n  712  \t        _react2.default.createElement(\n\n/Users/work/go/src/github.com/TimothyJones/chat-client/server/public/app.min-96ccab.js.map:\n    1: {\"version\":3,\"sources\":[\"webpack:///./index.js\",\"webpack:///./App.js\",\"webpack:///./actions/index.js\",\"webpack:////Users/work/go/src/g